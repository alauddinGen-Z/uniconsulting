{
  "name": "admitai-agent",
  "displayName": "AdmitAI Agent",
  "version": "0.0.1",
  "description": "Chrome Extension for autofilling university application forms using AI",
  "author": "UniConsulting Team",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.87.1",
    "lucide-react": "^0.561.0",
    "plasmo": "0.90.5",
    "postcss": "^8.5.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.22",
    "prettier": "3.2.4",
    "tailwindcss": "^3.4.0",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*",
      "http://localhost:*/*"
    ],
    "permissions": [
      "activeTab",
      "scripting",
      "storage",
      "tabs"
    ],
    "externally_connectable": {
      "matches": [
        "http://localhost:*/*",
        "https://*.vercel.app/*",
        "https://*.netlify.app/*"
      ]
    }
  }
}
